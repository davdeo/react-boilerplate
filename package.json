    "scripts": {
        "build": "webpack",
        "start": "webpack serve --open",
        "deploy": "npm run build && gh-pages -d dist",
        "check-eslint-config": "npx eslint-config-prettier src/index.tsx",
        "eslint-scan": "eslint src/{**/*,*}.{js,jsx,ts,tsx,md}",
        "eslint-fix": "eslint --fix src/{**/*,*}.{js,jsx,ts,tsx,md}",
        "check-stylelint-config": "stylelint-config-prettier-check",
        "stylelint-scan": "stylelint src/{**/*,*}.{css,less}",
        "stylelint-fix": "stylelint --fix src/{**/*,*}.{css,less}",
        "check-lint-config": "yarn check-eslint-config && yarn check-stylelint-config",
        "lint-scan": "yarn eslint-scan && yarn stylelint-scan",
        "lint-fix": "yarn eslint-fix && yarn stylelint-fix",
        "prepare": "husky install"
    },
    "lint-staged": {
        "{**/*,*}.{css,less}": "stylelint --fix",
        "{**/*,*}.{js,jsx,ts,tsx,md}": "eslint --cache --fix"
    },
